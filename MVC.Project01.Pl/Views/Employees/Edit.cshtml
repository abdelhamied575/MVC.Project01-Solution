@model Employee

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Employee</h1>
<br />
<br />

<form asp-action="Edit" method="post">

    <div asp-validation-summary="All" class="text-danger"></div>

    <partial name="PartialViews/EditCreateEmployeePartialView" model="Model" />


    <input type="submit" value="Save Changes" class="btn btn-primary" />
    <a asp-action="Index" asp-controller="Empoyees" class="btn btn-light">Back To List</a>



</form>

@section Validation {

    <partial name="_ValidationScriptsPartial" />


}

