
@model int


<td>

    <a asp-action="Details" class="btn btn-success" asp-route-id=@Model>
        <i class="fa-solid fa-eye"></i>
        Details
    </a>

</td>

<td>

    <a asp-action="Edit" class="btn btn-warning" asp-route-id=@Model>
        <i class="fa-solid fa-pen-to-square"></i>
        Update
    </a>

</td>

<td>

    <a asp-action="Delete" class="btn btn-danger" asp-route-id=@Model>
        <i class="fa-solid fa-trash"></i>
        Delete
    </a>

</td>
